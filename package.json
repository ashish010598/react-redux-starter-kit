{
  "name": "trago",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "cross-env npm run merge:api-json && cross-env NODE_ENV=development node internals/server",
    "start:qa": "cross-env NODE_ENV=development API_ENV=qa node internals/server",
    "host:web": "npm run test && npm run build:web:dev && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node internals/server",
    "build:clean-web": "rimraf ./build",
    "build": "cross-env npm run merge:api-json && npm run build:clean-web && cross-env NODE_ENV=production SERVER_ENV=dev GENERATE_SOURCEMAP=false webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build-test": "npm run build:clean-web && cross-env NODE_ENV=production API_ENV=qa SERVER_ENV=qa GENERATE_SOURCEMAP=false REACT_APP_ENV=QA webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build-prod-qa": "cross-env npm run merge:api-json && npm run build:clean-web && cross-env NODE_ENV=production GENERATE_SOURCEMAP=false SERVER_ENV=prod REACT_APP_ENV=PROD webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build-prod": "cross-env npm run merge:api-json && npm run build:clean-web && cross-env NODE_ENV=production SERVER_ENV=prod REACT_APP_ENV=PROD webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout && mkdir ./build/static",
    "generate": "cross-env APP_SHORT_NAME=trago plop --plopfile internals/generators/index.js",
    "analyze:web": "npm run merge:api-json && npm run build:clean-web && cross-env NODE_ENV=production webpack --config internals/webpack/webpack.test.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "merge:api-json": "node internals/scripts/mergeApiJsons.js",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage --detectOpenHandles --verbose",
    "test:staged": "cross-env CI=true NODE_ENV=test jest --detectOpenHandles --silent --findRelatedTests ",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll --detectOpenHandles --verbose",
    "prettify": "prettier --write",
    "lint": "npm run lint:js",
    "lint:eslint": "./node_modules/.bin/eslint --ignore-path ./.eslintignore",
    "lint:fix": "npm run lint:eslint -- --fix .",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "audit:lighthouse": "lighthouse http://localhost:3000/ --chrome-flags 'performance' --output html --output-path ./reports/lighthouse.html --view",
    "postinstall": "patch-package"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.10.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "express": "4.17.1",
    "express-favicon": "^2.0.1",
    "lodash": "4.17.20",
    "media-blender": "2.1.0",
    "offline-plugin": "5.0.7",
    "path": "^0.12.7",
    "react-date-range": "^1.0.3",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-redux-toastr": "7.6.5",
    "react-router-dom": "5.2.0",
    "react-test-renderer": "16.13.1",
    "redux": "4.0.5",
    "redux-persist": "6.0.0",
    "redux-saga": "1.1.3",
    "sanitize.css": "11.0.1",
    "smoothscroll-polyfill": "^0.4.3",
    "autoprefixer": "9.8.0",
    "bowser": "2.9.0",
    "chalk": "4.0.0",
    "circular-dependency-plugin": "5.2.0",
    "compression": "1.7.4",
    "compression-webpack-plugin": "4.0.0",
    "cross-env": "7.0.2",
    "css-loader": "3.5.3",
    "enzyme-to-json": "3.5.0",
    "file-loader": "6.0.0",
    "hoist-non-react-statics": "3.3.2",
    "html-loader": "1.1.0",
    "html-webpack-plugin": "4.3.0",
    "image-webpack-loader": "6.0.0",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "lint-staged": "10.2.7",
    "minimist": "1.2.5",
    "node-sass": "4.14.1",
    "plop": "2.7.1",
    "postcss-loader": "3.0.0",
    "pre-commit": "1.2.2",
    "prettier": "2.0.5",
    "pretty": "2.0.0",
    "react-app-polyfill": "1.0.6",
    "rimraf": "3.0.2",
    "sass-loader": "8.0.2",
    "style-loader": "1.2.1",
    "superagent": "5.2.2",
    "svg-url-loader": "6.0.0",
    "svgo": "1.3.2",
    "svgo-loader": "2.2.1",
    "terser-webpack-plugin": "3.0.2",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-pwa-manifest": "4.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/cli": "7.10.1",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.10.1",
    "@babel/plugin-transform-react-constant-elements": "7.10.1",
    "@babel/plugin-transform-react-inline-elements": "7.10.1",
    "@babel/polyfill": "7.10.1",
    "@babel/preset-react": "7.10.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "24.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "eslint": "7.1.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-webpack": "0.12.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "eslint-plugin-redux-saga": "1.1.3",
    "lighthouse": "5.6.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "jest": "24.1.0",
    "jest-cli": "24.1.0",
    "ngrok": "3.2.7"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "browserslist": [
    ">0.2%",
    "not dead",
    "ie <= 11",
    "op_mini all"
  ]
}
